<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Crossstreet</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Category</th>
    <th>Description</th>
    <th>Url</th>
    <th>Avgprice</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.address %></td>
    <td><%= place.crossStreet %></td>
    <td><%= place.city %></td>
    <td><%= place.state %></td>
    <td><%= place.zip %></td>
    <td><%= place.category %></td>
    <td><%= place.description %></td>
    <td><%= place.url %></td>
    <td><%= place.avgPrice %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
